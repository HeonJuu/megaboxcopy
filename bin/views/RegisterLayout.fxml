			<font>
				<Font name="Expo M" size="22.0" />
			</font>
		</Button>
		<Button id="seven" layoutX="618.0" layoutY="509.0"
			mnemonicParsing="false" onAction="#cancel" prefHeight="53.0"
			prefWidth="148.0" text="CANCEL" textFill="WHITE">
			<font>
				<Font name="Expo M" size="22.0" />
			</font>
		</Button>
		<TextField fx:id="txtId" layoutX="240.0" layoutY="190.0"
			prefHeight="62.0" prefWidth="320.0" text="아이디 입력" />
		<PasswordField fx:id="pass" layoutX="240.0"
			layoutY="300.0" prefHeight="62.0" prefWidth="320.0" />
		<Label id="eight" alignment="CENTER" layoutX="339.0"
			layoutY="102.0" prefHeight="29.0" prefWidth="122.0" text="REGISTER"
			textAlignment="CENTER" textFill="WHITE">
			<font>
				<Font name="Expo M" size="24.0" />
